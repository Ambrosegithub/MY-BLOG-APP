<%= form_with model: @new, method: :post, local: true do |f| %>
<div>
  <%= f.label :Post_title, require: true %><br>
  <%= f.text_field :Title %>
</div>
<div>
  <%= f.label :Enter_Text %><br>
  <%= f.text_area :Text, rows: 10, require: true %>
  
</div>
<div>
  <%= f.label :submit %><br>
  <%= f.submit :createpost %>
</div>

<% end %>


